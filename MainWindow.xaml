                            Content = "0"
                            HorizontalAlignment = "Right"
                            Margin = "0,70,0,0"
                            VerticalAlignment = "Top"
                            Background = "{x:Null}"
                            Foreground = "#FF9B00"
                            FontWeight="Bold"
                            FontFamily = "Segoe UI"/>

                        <Label 
                            Content="Y-Axis" 
                            FontFamily="Segoe UI Semilight" 
                            VerticalAlignment="Top" 
                            Margin="0,100,0,0"
                            HorizontalAlignment="Left"/>

                        <Label 
                            x:Name="ScreenYAxisText"
                            Content = "0"
                            HorizontalAlignment = "Right"
                            Margin = "0,100,0,0"
                            VerticalAlignment = "Top"
                            Background = "{x:Null}"
                            Foreground = "#FF9B00"
                            FontWeight="Bold"
                            FontFamily = "Segoe UI"/>

                    </Grid>

                </Grid>

            </TabItem>

        </dragablz:TabablzControl>
    </Grid>





