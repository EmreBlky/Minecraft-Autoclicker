                        <Rectangle 
                            x:Name="MouseStatusRectangle"
                            HorizontalAlignment="Left" 
                            Height="24" 
                            Margin="1,0,0,0" 
                            VerticalAlignment="Top" 
                            Width="100" 
                            Fill="#FF9B00"
