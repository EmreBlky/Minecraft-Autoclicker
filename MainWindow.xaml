            <TabItem Header="STATUS">
                <Grid>
                    <Grid x:Name="ClickerStatusGrid" 
                          HorizontalAlignment="Left" 
                          Height="130" 
                          Margin="65,45,0,0" 
                          VerticalAlignment="Top" 
                          Width="200">

                        <Label 
                                x:Name="LeftClickerStatusText"
                                Content = "DISABLED"
                                HorizontalAlignment = "Right"
                                Margin = "0,40,0,0"
                                VerticalAlignment = "Top"
                                Background = "{x:Null}"
                                Foreground = "#FF9B00"
                                FontWeight="Bold"
                                FontFamily = "Segoe UI"/>

                        <Rectangle 
                                x:Name="StatusRectangle"
                                HorizontalAlignment="Left" 
                                Height="24" 
                                Margin="1,0,0,0" 
                                VerticalAlignment="Top" 
                                Width="120" 
                                Fill="#FF9B00"
                                ToolTipService.Placement="Right"/>

                        <Label
                                x:Name="StatusLabel"
                                Content="CLICKER STATUS" 
                                HorizontalAlignment="Left"
                                Margin="11,0,0,0"
                                VerticalAlignment="Top"
                                Background="{x:Null}"
                                Foreground="#FF150909"
                                FontFamily="Segoe UI Semibold"/>

                        <Label
                                Content="Left Clicker" 
                                FontFamily="Segoe UI Semilight"
                                VerticalAlignment="Top"
                                Margin="0,40,0,0"
                                HorizontalAlignment="Left"/>

                        <Label
                                Content="Right Clicker" 
                                FontFamily="Segoe UI Semilight"
                                VerticalAlignment="Top"
                                Margin="0,70,0,0" 
                                HorizontalAlignment="Left"/>

                        <Label 
                                x:Name="RightClickerStatusText"
                                Content = "DISABLED"
