<controls:MetroWindow 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:av="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="av"
        x:Class="Autoclicker.MainWindow"
        ResizeMode="NoResize"
        Title=""
        Height="600"
        Width="1200"
        WindowStartupLocation="CenterScreen"
        Loaded="MetroWindow_Loaded" Closing="MetroWindow_Closing">

    <Grid Background="#FF1E1E1E" Margin="0,0,0,1">

        <dragablz:TabablzControl 
                x:Name="TabablzControl"
                FixedHeaderCount="4"
                TabStripPlacement="Top" 
                FocusVisualStyle="{x:Null}"
                Background="#FF1E1E1E" 
                BorderBrush="#FF8D00"
                Foreground="Black" IsSynchronizedWithCurrentItem="False">

            <controls:TabControlHelper.UnderlineBrush>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black"/>
                    <GradientStop Color="#FF11E231" Offset="1"/>
                </LinearGradientBrush>
            </controls:TabControlHelper.UnderlineBrush>

            <TabItem Header="RIGHT CLICKER" Foreground="{DynamicResource TextBrush}">
                <Grid>

                    <Grid x:Name="RightClickerMainGrid" 
                              HorizontalAlignment="Left" 
                              Height="350" 
                              Margin="65,45,0,0" 
                              VerticalAlignment="Top" 
                              Width="290">

                        <Rectangle 
                                x:Name="RightButtonRectangle" 
                                HorizontalAlignment="Left" 
                                Height="24" 
                                VerticalAlignment="Top" 
                                Width="100" 
                                Fill="#FF9B00"
                                ToolTipService.Placement="Right"/>


                        <Label 
                                x:Name="RightButtonLabel"
                                Content = "RIGHT BUTTON"
                                HorizontalAlignment = "Left"
                                Margin = "5,0,0,0"
                                VerticalAlignment = "Top"
                                Background = "{x:Null}"
                                FontFamily = "Segoe UI Semibold"
                                ToolTip="Right Mouse Button AutoClicker."
                                Foreground = "#FF150909"/>

                        <Label
                                Content="Clicks per second"
                                HorizontalAlignment="Left"
                                Margin="0,40,0,0"
                                VerticalAlignment="Top"
                                FontFamily="Segoe UI Semilight"/>

                        <Slider
                                x:Name = "RightCpsSlider"
                                HorizontalAlignment="Center"
                                Margin="50,45,0,0"
                                VerticalAlignment="Top"
                                Width="120"
                                Value="10"
                                Minimum="5"
                                Maximum="500" 
                                Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                Background="#FF1B191B"
                                BorderBrush="#FF2F2C2F"
                                Foreground="#FF9B00"/>

                        <Label 
                                x:Name = "RightCpsText"
                                FontSize="11"
                                Content="10 CPS"
                                HorizontalAlignment="Right"
                                Margin="0,40,0,0"
                                VerticalAlignment="Top"
                                Background="{x:Null}"
                                Foreground="#FFE3E3E3"
                                FontFamily="Segoe UI Semibold"/>

                        <Button 
                                x:Name="RightClickerButton"
                                HorizontalAlignment="Right"
                                Margin="150,0,0,0"
                                VerticalAlignment="Top"
                                Width="76" 
                                Height="24" 
                                Content="BIND A KEY" 
                                FontSize="8" 
                                Background="#FF9B00" 
                                BorderBrush="{x:Null}" />

                        <CheckBox
                                x:Name="RightClickerCheckbox"
                                HorizontalAlignment="Left" 
                                Margin="115,3,0,0" 
                                VerticalAlignment="Top"/>
                    </Grid>

                    <Grid x:Name="RightClickerBoundsGrid"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Margin="65, 0, 0, 100"
                              Width="290">

                        <Rectangle 
                                        x:Name="RightButtonBoundsRectangle" 
                                        HorizontalAlignment="Left" 
                                        Height="24" 
                                        VerticalAlignment="Top" 
                                        Width="69" 
                                        Fill="#FF9B00"
                                        ToolTip="LEFT MOUSE BUTTON CLICKER."
                                        ToolTipService.Placement="Right"
                                        Margin="0,0,0,0"/>

                        <Label 
                                        x:Name="LeftBoundsLabel"
                                        Content = "BOUNDS"
                                        HorizontalAlignment = "Left"
                                        Margin = "6,0,0,0"
                                        VerticalAlignment = "Top"
                                        Background = "{x:Null}"
                                        Foreground = "#FF150909"
                                        FontFamily = "Segoe UI Semibold"/>

                        <Label
                                        Content="Lower Bound"
                                        HorizontalAlignment="Left"
                                        Margin="0,40,0,0"
                                        VerticalAlignment="Top"
                                        ToolTip="Tip: For smaller values, use the arrow keys to increase the slider."
                                        FontFamily="Segoe UI Semilight"/>

                        <Slider
                                        x:Name = "RightLowerCpsSlider"
                                        HorizontalAlignment="Center"
                                        Margin="0,45,0,0"
                                        VerticalAlignment="Top"
                                        Width="100"
                                        Minimum="0"
                                        Maximum="9" 
                                        Value="3"
                                        Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                        Background="#FF1B191B"
                                        BorderBrush="#FF2F2C2F"
                                        Foreground="#FF9B00"/>

                        <Label 
                                        x:Name = "RightLowerCpsText"
                                        FontSize="11"
                                        Content="3 LESS CPS"
                                        HorizontalAlignment="Right"
                                        Margin="0,40,0,0"
                                        VerticalAlignment="Top"
                                        Background="{x:Null}"
                                        Foreground="#FFE3E3E3"
                                        FontFamily="Segoe UI Semibold"/>

                        <Label
                                        Content="Upper Bound"
                                        HorizontalAlignment="Left"
                                        Margin="0,70,0,0"
                                        VerticalAlignment="Top"
                                        ToolTip="Tip: For smaller values, use the arrow keys to increase the slider."
                                        FontFamily="Segoe UI Semilight"/>

                        <Slider
                                        x:Name = "RightUpperCpsSlider"
                                        HorizontalAlignment="Center"
                                        Margin="0,75,0,0"
                                        VerticalAlignment="Top"
                                        Width="100"
                                        Maximum="20"
                                        Minimum="0"
                                        Value="1.5"
                                        Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                        Background="#FF1B191B"
                                        BorderBrush="#FF2F2C2F"
                                        Foreground="#FF9B00"/>

                        <Label 
                                        x:Name = "RightUpperCpsText"
                                        FontSize="11"
                                        Content="1.5 MORE CPS"
                                        HorizontalAlignment="Right"
                                        Margin="0,70,0,0"
                                        VerticalAlignment="Top"
                                        Background="{x:Null}"
                                        Foreground="#FFE3E3E3"
                                        FontFamily="Segoe UI Semibold"/>

                    </Grid>

                    <Grid x:Name="RightClickerCpsDropGrid"
                              VerticalAlignment="Bottom"
                              HorizontalAlignment="Left"
                              Margin="65,0, 0, 130"
                              Width="290">

                        <Rectangle 
                                        x:Name="RightButtonCpsDropRectangle"
                                        HorizontalAlignment="Left" 
                                        Height="24" 
                                        Margin="0,0,0,0" 
                                        VerticalAlignment="Top" 
                                        Width="75" 
                                        Fill="#FF9B00"
                                        ToolTip="LEFT MOUSE BUTTON CLICKER."
                                        ToolTipService.Placement="Right"/>

                        <Label
                                        x:Name="CpsDropLabel"
                                        Content="CPS DROP"
                                        HorizontalAlignment="Left"
                                        Margin="7,0,0,0"
                                        VerticalAlignment="Top"
                                        Background="{x:Null}"
                                        Foreground="#FF150909"
                                        ToolTip="Set the amount of cps drop and the probability of the drop happening."
                                        FontFamily="Segoe UI Semibold"/>

                        <Label 
                                        Content="Amount" 
                                        FontFamily="Segoe UI Semilight" 
                                        VerticalAlignment="Top" 
                                        Margin="0,40,0,0" 
                                        HorizontalAlignment="Left"/>

                        <Slider 
                                        x:Name="RightCpsDropAmountSlider" 
                                        Margin="0,45,0,0" 
                                        VerticalAlignment="Top" 
                                        Maximum="30"
                                        HorizontalAlignment="Center" 
                                        Width="100" 
                                        Value="2.5"
                                        Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                        Background="#FF1B191B"
                                        BorderBrush="#FF2F2C2F"
                                        Foreground="#FF9B00"/>

                        <Label 
                                        x:Name="RightCpsDropAmountText"
                                        Content="2.5 CPS" 
                                        HorizontalAlignment="Right" 
                                        Margin="0,40,0,0" 
                                        VerticalAlignment="Top" 
                                        Background="{x:Null}" 
                                        Foreground="#FFE8E8E8" 
                                        FontFamily="Segoe UI Semibold"/>

                        <Label 
                                        Content="Probability" 
                                        HorizontalAlignment="Left" 
                                        Margin="0,70,0,0" 
                                        VerticalAlignment="Top" 
                                        FontFamily="Segoe UI Semilight"/>

                        <Slider 
                                        x:Name="RightCpsDropProbabilitySlider"
                                        HorizontalAlignment="Center" 
                                        Margin="0,75,0,0" 
                                        VerticalAlignment="Top"
                                        Width="100" 
                                        Minimum="0"
                                        Maximum="100" 
                                        Value="22"
                                        Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                        Background="#FF1B191B"
                                        BorderBrush="#FF2F2C2F"
                                        Foreground="#FF9B00"/>

                        <Label 
                                        x:Name="RightCpsDropProbabilityText"
                                        Content="22 %" 
                                        HorizontalAlignment="Right" 
                                        Margin="0,70,0,0" 
                                        VerticalAlignment="Top" 
                                        Background="{x:Null}" 
                                        Foreground="#FFE8E8E8" 
                                        FontFamily="Segoe UI Semibold"/>



                    </Grid>


                    <Grid x:Name="JitterGrid" 
                          HorizontalAlignment="Center" 
                          VerticalAlignment="Top" 
                          Width="290" 
                          Margin="70,45,0,0">

                        <Rectangle 
                                x:Name = "RightButtonJitterRectangle"
                                HorizontalAlignment="Left" 
                                Height="24" 
                                VerticalAlignment="Top" 
                                Width="53" 
                                Fill="#FF9B00"
                                ToolTip="LEFT MOUSE BUTTON CLICKER."
                                ToolTipService.Placement="Right"/>

                        <Label 
                                x:Name="JitterLabel"
                                Content="JITTER" 
                                HorizontalAlignment="Left" 
                                Margin="5,0,0,0" 
                                VerticalAlignment="Top" 
                                Background="{x:Null}" 
                                Foreground="#FF150909" 
                                FontFamily="Segoe UI Semibold" 
                                ToolTip="Jitter motion."
                                FontWeight="Bold"/>

                        <Slider 
                                x:Name = "RightClickerJitterXAxis"
                                HorizontalAlignment="Center" 
                                Margin="0,45,0,0"
                                VerticalAlignment="Top" 
                                Width="150" 
                                Maximum="30"
                                Minimum="0" Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                Background="#FF1B191B"
                                BorderBrush="#FF2F2C2F"
                                Foreground="#FF9B00"/>

                        <Label 
                                Content="X-Axis" 
                                HorizontalAlignment="Left" 
                                Margin="0,40,0,0"
                                VerticalAlignment="Top"
                                Background="{x:Null}" 
                                Foreground="#DDFFFFFF" 
                                FontFamily="Segoe UI"/>

                        <Label 
                                x:Name="RightXAxisText"
                                Content="0 SHIFT" 
                                HorizontalAlignment="Right" 
                                Margin="0,40,0,0" 
                                VerticalAlignment="Top" 
                                Background="{x:Null}" 
                                Foreground="#FFE8E8E8" 
                                FontFamily="Segoe UI Semibold"/>

                        <Label 
                                Content="Y-Axis" 
                                HorizontalAlignment="Left" 
                                Margin="0,70,0,0" 
                                VerticalAlignment="Top" 
                                Background="{x:Null}" 
                                Foreground="#DDFFFFFF" 
                                FontFamily="Segoe UI"/>

                        <Slider 
                                x:Name = "RightClickerJitterYAxis" 
                                HorizontalAlignment="Center" 
                                Margin="0,75,0,0" 
                                VerticalAlignment="Top" 
                                Width="150" 
                                Maximum="30"
                                Minimum="0" Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                Background="#FF1B191B"
                                BorderBrush="#FF2F2C2F"
                                Foreground="#FF9B00"/>

                        <Label 
                                x:Name="RightYAxisText"
                                Content="0 SHIFT" 
                                HorizontalAlignment="Right" 
                                Margin="0,70,0,0" 
                                VerticalAlignment="Top" 
                                Background="{x:Null}" 
                                Foreground="#FFE8E8E8" 
                                FontFamily="Segoe UI Semibold"/>

                        <CheckBox
                                x:Name="RightJitterCheckbox"
                                HorizontalAlignment="Left" 
                                Margin="70,3,0,0" 
                                VerticalAlignment="Top"/>
                    </Grid>

                    <Grid
                        x:Name="RightCpsDelimiter"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Margin="75, 0, 0, 130"
                        Width="290">

                        <Rectangle 
                            x:Name="RightCpsDelimiterRectangle" 
                            HorizontalAlignment="Left" 
                            Height="24" 
                            VerticalAlignment="Top" 
                            Width="100" 
                            Fill="#FF9B00"
                            ToolTip="Allows you to set a maximum number of cps."
                            ToolTipService.Placement="Right"/>

                        <Label 
                            x:Name="RightCpsDelimiterLabel"
                            Content = "CPS DELIMITER"
                            ToolTip="Set the maximum number of cps (up to 500 cps)"
                            HorizontalAlignment = "Left"
                            Margin = "6,0,0,0"
                            VerticalAlignment = "Top"
                            Background = "{x:Null}"
                            Foreground = "#FF150909"
                            FontFamily = "Segoe UI Semibold"/>

                        <Slider 
                            x:Name="RightCpsDelimiterSlider" 
                            Margin="0,45,0,0" 
                            VerticalAlignment="Top" 
                            HorizontalAlignment="Left"
                            Width="190"
                            TickFrequency="10"
                            Maximum="500" 
                            Minimum="30" 
                            Value="30"
                            SmallChange="1" 
                            Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                            Background="#FF1B191B"
                            BorderBrush="#FF2F2C2F"
                            Foreground="#FF9B00"/>

                        <Label 
                            x:Name="RightCpsDelimiterText"
                            Content="30 MAX CPS" 
                            HorizontalAlignment="Right" 
                            Margin="0,40,0,0" 
                            VerticalAlignment="Top" 
                            Background="{x:Null}" 
                            Foreground="#FFE8E8E8" 
                            FontFamily="Segoe UI Semibold"/>

                    </Grid>

                </Grid>


            </TabItem>

            <TabItem Header="LEFT CLICKER">

                <Grid>
                    <Grid x:Name="BreakBlocksGrid" 
                          HorizontalAlignment="Right" 
                          Height="24" 
                          Margin="0,45,50,0" 
                          VerticalAlignment="Top" 
                          Width="240">

                        <CheckBox 
                            x:Name="BreakBlocksCheckbox"
                            Content=""
                            HorizontalAlignment="Left"
                            Margin="105,0,0,0"
                            VerticalAlignment="Center"
                            Foreground="#FFE8E8E8"
                            Background="#FF9B00"
                            BorderBrush="#FF9B00"/>

                        <Rectangle 
                            x:Name="LeftButtonBreakBlocksRectangle" 
                            HorizontalAlignment="Left" 
                            Height="24" 
                            VerticalAlignment="Center" 
                            Width="100" 
                            Fill="#FF9B00"
                            ToolTip="Allows you to break blocks while the left autoclicker is active."
                            ToolTipService.Placement="Right"/>

                        <Label 
                                x:Name="BreakBlocksLabel"
                            Content="BREAK BLOCKS"
                            HorizontalAlignment="Left"
                            Margin="5,0,0,0"
                            VerticalAlignment="Center"
                            Background="{x:Null}"
                            Foreground="#FF150909"
                            FontFamily="Segoe UI Semibold"/>

                    </Grid>

                    <Grid
                        x:Name="CpsDelimiter"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Bottom"
                        Margin="0, 0, 50, 162"
                        Width="240">

                        <Rectangle 
                            x:Name="CpsDelimiterRectangle" 
                            HorizontalAlignment="Left" 
                            Height="24" 
                            VerticalAlignment="Top" 
                            Width="100" 
                            Fill="#FF9B00"
                            ToolTip="Allows you to set a maximum number of cps."
                            ToolTipService.Placement="Right"/>

                        <Label 
                            x:Name="CpsDelimiterLabel"
                            Content = "CPS DELIMITER"
                            HorizontalAlignment = "Left"
                            Margin = "6,0,0,0"
                            VerticalAlignment = "Top"
                            Background = "{x:Null}"
                            Foreground = "#FF150909"
                            FontFamily = "Segoe UI Semibold"/>

                        <Slider 
                            x:Name="CpsDelimiterSlider" 
                            Margin="0,44,0,0" 
                            VerticalAlignment="Top" 
                            HorizontalAlignment="Left"
                            Width="140"
                            TickFrequency="10"
                            Maximum="500" 
                            Minimum="30" 
                            Value="1.5"
                            SmallChange="1" 
                            Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                            Background="#FF1B191B"
                            BorderBrush="#FF2F2C2F"
                            Foreground="#FF9B00"/>

                        <Label 
                            x:Name="CpsDelimiterText"
                            Content="30 CPS" 
                            HorizontalAlignment="Right" 
                            Margin="0,38,0,0" 
                            VerticalAlignment="Top" 
                            Background="{x:Null}" 
                            Foreground="#FFE8E8E8" 
                            FontFamily="Segoe UI Semibold"/>



                    </Grid>

                    <Grid x:Name="BlockhitGrid" 
                          Margin="70,0,0,100" 
                          Width="290"
                          VerticalAlignment="Center"
                          HorizontalAlignment="Center">

                        <Slider 
                                x:Name="LeftBlockhitProbabilitySlider" 
                                Minimum="0"
                                Maximum="100" 
                                Width="100"
                                VerticalAlignment="Top"
                                HorizontalAlignment="Center"
                                Margin="40,45,38,0" 
                                Value="25"
                                Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                Background="#FF1B191B"
                                BorderBrush="#FF2F2C2F"
                                Foreground="#FF9B00"/>

                        <Slider 
                                x:Name="BlockhitDelaySlider" 
                                Minimum="1"
                                Maximum="1000" 
                                Width="100"
                                VerticalAlignment="Top"
                                HorizontalAlignment="Center"
                                Margin="40,75,38,0" 
                                Value="500"
                                Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                                Background="#FF1B191B"
                                BorderBrush="#FF2F2C2F"
                                Foreground="#FF9B00"/>

                        <Label 
                                x:Name="BlockhitText"
                                Content="25 %" 
                                Background="{x:Null}" 
                                Foreground="#FFE8E8E8" 
                                FontFamily="Segoe UI Semibold" 
                                VerticalAlignment="Top" 
                                Margin="190,32,0,0" 
                                HorizontalAlignment="Right"/>

                        <Label 
                                x:Name="BlockhitDelayText"
                                Content="500 MS" 
                                Background="{x:Null}" 
                                Foreground="#FFE8E8E8" 
                                FontFamily="Segoe UI Semibold" 
                                VerticalAlignment="Top" 
                                Margin="190,60,0,0" 
                                HorizontalAlignment="Right"/>

                        <Label 
                                Content="Probability" 
                                FontFamily="Segoe UI Semilight" 
                                VerticalAlignment="Top" 
                                Margin="0,40,0,0" 
                                HorizontalAlignment="Left"/>

                        <Label 
                                Content="Delay" 
                                FontFamily="Segoe UI Semilight" 
                                VerticalAlignment="Top" 
                                Margin="0,70,0,0" 
                                HorizontalAlignment="Left"/>

                        <Rectangle 
                                x:Name="LeftButtonBlockhitRectangle" 
                                HorizontalAlignment="Left" 
                                Height="24" 
                                VerticalAlignment="Top" 
                                Width="75" 
                                Fill="#FF9B00"
                                ToolTip="Allows you to block the other player's hits."
                                ToolTipService.Placement="Right"/>

                        <Label
                                x:Name="BlockhitLabel"
                                Content="BLOCKHIT"
                                Background="{x:Null}"
                                Foreground="#FF150909"
                                FontFamily="Segoe UI Semibold"
                                VerticalAlignment="Top"
                                Margin="4,0,0,0"
                                HorizontalAlignment="Left"/>

                        <CheckBox
                x:Name="BlockhitCheckbox"
                Content="" 
                HorizontalAlignment="Left" 
                HorizontalContentAlignment="Center"
                Margin="90,3,0,0" 
                VerticalAlignment="Top"/>
                    </Grid>

                    <Grid x:Name="InventoryGrid" 
                          HorizontalAlignment="Right" 
                          Height="62" 
                          Margin="0,170,50,0" 
                          VerticalAlignment="Top" 
                          Width="240">

                        <Label 
                Content="Wait time" 
                HorizontalAlignment="Left" 
                Margin="0,38,0,0" 
                VerticalAlignment="Top" 
                Background="{x:Null}" 
                Foreground="#FFE8E8E8" 
                FontFamily="Segoe UI Semilight"/>

                        <Slider 
                x:Name="InventoryDelaySlider" 
                Margin="76,44,70,0" 
                VerticalAlignment="Top" 
                TickFrequency="10"
                Maximum="5" 
                Minimum="1" 
                Value="1.5"
                SmallChange="1" 
                Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                Background="#FF1B191B"
                BorderBrush="#FF2F2C2F"
                Foreground="#FF9B00"/>

                        <Label 
                x:Name="InventoryDelayText"
                Content="1.5 SEC" 
                HorizontalAlignment="Left" 
                Margin="168,38,0,0" 
                VerticalAlignment="Top" 
                Background="{x:Null}" 
                Foreground="#FFE8E8E8" 
                FontFamily="Segoe UI Semibold"/>
                        <CheckBox 
                x:Name="DisableWhenInventoryOpenCheckbox"
                Content=""
                Foreground="#FFE8E8E8"
                Background="#FF9B00"
                BorderBrush="#FF9B00" VerticalAlignment="Top" Margin="211,3,0,0" HorizontalAlignment="Left"/>
                        <Rectangle 
                                x:Name="LeftButtonInventoryRectangle" 
                                Height="24" 
                                Width="206" 
                                Fill="#FF9B00"
                                ToolTip="Allows you to temporarily disable the autoclicker while the inventory is open for a certain configurable length of time."
                                ToolTipService.Placement="Right" 
                                VerticalAlignment="Top" 
                                HorizontalAlignment="Left"/>
                        <Label
                                x:Name="DisableWhenInventoryLabel"
                                Content="DISABLE WHEN INVENTORY OPEN"
                                Background="{x:Null}"
                                Foreground="#FF150909"
                                FontFamily="Segoe UI Semibold"
                                VerticalAlignment="Top" 
                                Margin="4,0,0,0" 
                                HorizontalAlignment="Left"/>
                    </Grid>

                    <Grid x:Name="RefillGrid" 
                          HorizontalAlignment="Center" 
                          Width="290"
                          Margin="70,0,0,70" 
                          VerticalAlignment="Bottom" >

                        <Slider 
                x:Name="RefillSpeedSlider"
                HorizontalAlignment="Center" 
                Margin="0,45,0,0" 
                VerticalAlignment="Top"
                Width="90" 
                Value="30"
                Minimum="1"
                Maximum="100" 
                Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                Background="#FF1B191B"
                BorderBrush="#FF2F2C2F"
                Foreground="#FF9B00"/>

                        <Label 
                x:Name="RefillText"
                Content="30 VELOCITY" 
                HorizontalAlignment="Right" 
                Margin="0,40,0,0" 
                VerticalAlignment="Top" 
                Background="{x:Null}" 
                Foreground="#FFE8E8E8" 
                FontFamily="Segoe UI Semibold"/>

                        <Label 
                Content="Mouse Speed"
                HorizontalAlignment="Left"
                Margin="0,40,0,0"
                VerticalAlignment="Top"
                FontFamily="Segoe UI Semilight"/>

                        <Label 
                Content="X-Axis"
                HorizontalAlignment="Left"
                Margin="2,70,0,0"
                VerticalAlignment="Top"
                FontFamily="Segoe UI Semilight"/>

                        <Slider 
                x:Name="RefillXAxisRandomSlider"
                HorizontalAlignment="Center" 
                Margin="0,75,0,0" 
                VerticalAlignment="Top"
                Width="90" 
                Value="5.7"
                Minimum="1"
                Maximum="100" 
                Style="{DynamicResource MahApps.Metro.Styles.FlatSlider}"
                Background="#FF1B191B"
                BorderBrush="#FF2F2C2F"
                Foreground="#FF9B00"/>

                        <Label
                x:Name="RefillXAxisText"
                Content="5.7 SHIFT" 
                HorizontalAlignment="Right" 
                Margin="0,70,0,0" 
                VerticalAlignment="Top" 
                Background="{x:Null}" 
                Foreground="#FFE8E8E8" 
                FontFamily="Segoe UI Semibold"/>

                        <Slider 
                x:Name="RefillYAxisRandomSlider"
                HorizontalAlignment="Center" 
                Margin="0,105,0,0" 
                VerticalAlignment="Top"
                Width="90" 
                Maximum="100"
                Minimum="1"
                Value="5" 
                Style="{DynamicResource FlatSlider}"
                Background="#FF1B191B"
                BorderBrush="#FF2F2C2F"
                Foreground="#FF9B00"/>

                        <CheckBox
                x:Name="RefillSpeedCheckbox"
                HorizontalAlignment="Left"
                Margin="67,3,0,0"
                VerticalAlignment="Top"/>

                        <Label 
                x:Name="RefillYAxisText"
                Content="5 SHIFT" 
                HorizontalAlignment="Right" 
                Margin="0,100,0,0" 
                VerticalAlignment="Top" 
                Background="{x:Null}" 
                Foreground="#FFE8E8E8" 
                FontFamily="Segoe UI Semibold"/>

                        <Label
                Content="Y-Axis"
                HorizontalAlignment="Left"
                Margin="2,100,0,0"
                VerticalAlignment="Top"
                FontFamily="Segoe UI Semilight"/>

                        <Label
                Content="Steps"
                HorizontalAlignment="Left"
