                                        Height="24" 
                                        Content="BIND A KEY" 
                                        FontSize="8" 
                                        Background="#FF9B00"
                                        BorderBrush="{x:Null}" />

                        <Button 
                                        x:Name="ButterflyButton"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Margin="130,90,0,0"
                                        Width="83" 
                                        Height="24" 
                                        Content="BIND A KEY" 
                                        FontSize="8" 
