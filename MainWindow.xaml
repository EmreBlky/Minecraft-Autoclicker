                                Margin = "0,70,0,0"
                                VerticalAlignment = "Top"
                                FontWeight="Bold"
                                Background = "{x:Null}"
                                Foreground = "#FF9B00"
                                FontFamily = "Segoe UI"/>

                    </Grid>

                    <Grid x:Name="MinecraftStatus"
                          VerticalAlignment="Center"
                          HorizontalAlignment="Left"
                          Width="200"
                          Margin="65, 0, 0, 100">

                        <Rectangle 
                            x:Name="MinecraftStatusRectangle"
                            HorizontalAlignment="Left" 
                            Height="24" 
                            Margin="1,0,0,0" 
                            VerticalAlignment="Top" 
                            Width="140" 
                            Fill="#FF9B00"
                            ToolTip="Displays whether a Minecraft instance is running."
                            ToolTipService.Placement="Right"/>

                        <Label
                            x:Name="MinecraftStatusLabel"
                            Content="MINECRAFT STATUS" 
                            HorizontalAlignment="Left"
                            Margin="11,0,0,0"
                            VerticalAlignment="Top"
                            Background="{x:Null}"
                            Foreground="#FF150909"
                            FontFamily="Segoe UI Semibold"/>

                        <Label 
                            Content="Is Minecraft Open" 
                            FontFamily="Segoe UI Semilight" 
                            VerticalAlignment="Top" 
                            Margin="0,40,0,0"
                            HorizontalAlignment="Left"/>

                        <Label 
                            x:Name="IsMinecraftOpenStatusText"
                            Content = "FALSE"
                            HorizontalAlignment = "Right"
                            Margin = "0,40,0,0"
                            VerticalAlignment = "Top"
                            Background = "{x:Null}"
                            Foreground = "#FF9B00"
                            FontWeight="Bold"
                            FontFamily = "Segoe UI"/>


                    </Grid>

                    <Grid x:Name="MouseStatus"
                          HorizontalAlignment="Left"
                          VerticalAlignment="Bottom"
