                            ToolTipService.Placement="Right"/>

                        <Label
                            x:Name="MouseStatusLabel"
                            Content="MOUSE STATUS" 
                            HorizontalAlignment="Left"
                            Margin="2,0,0,0"
                            VerticalAlignment="Top"
                            Background="{x:Null}"
                            Foreground="#FF150909"
                            FontFamily="Segoe UI Semibold"/>

                        <CheckBox 
                            x:Name="ToggleMouseStatus"
                            HorizontalAlignment="Left"
                            Margin="120,3,0,0"
                            IsChecked="False"
                            VerticalAlignment="Top"
                            Foreground="#DDFFFFFF"
                            Background="#FF9B00"
                            BorderBrush="#FF9B00"/>

                        <Label 
                            Content="Simulated click detected" 
                            FontFamily="Segoe UI Semilight" 
                            VerticalAlignment="Top" 
                            Margin="0,40,0,0"
                            HorizontalAlignment="Left"/>

                        <Label 
                            x:Name="SimulatedClickStatusText"
                            Content = "FALSE"
